<% provide(:title, "Edit Apprenticeship") %> 
<h1>Update your apprenticeship</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@apprenticeship) do |f| %>
    <%= render 'fields', f: f %>

    <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>